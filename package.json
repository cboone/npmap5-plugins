{
    "name": "maplibre-gl-arcgis-rest-source",
    "exportName": "ArcGisRestSource",
    "version": "0.0.1",
    "description": "Allows ArcGIS REST sources to be used with Maplibre as Raster Tiles",
    "keywords": [
        "maplibre",
        "npmap",
        "nps",
        "arcgis-rest"
    ],
    "author": "James McAndrew  <james_mcandrew@partner.nps.gov>",
    "license": "MIT",
    "main": "dist/maplibre-gl-arcgis-rest-source.js",
    "types": "dist/src/index.d.ts",
    "module": "dist/maplibre-gl-arcgis-rest-source.esm.js",
    "browser": "dist/maplibre-gl-arcgis-rest-source.min.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf ./dist",
        "build-dev": "NODE_ENV=development npm run build",
        "watch-dev": "NODE_ENV=development npm run watch",
        "build-prod": "NODE_ENV=production npm run build",
        "build": "npm run clean && rollup -c rollup.config.js",
        "watch": "rollup -c rollup.config.js --watch"
    },
    "devDependencies": {
        "@open-wc/building-rollup": "^2.2.1",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/arcgis-rest-api": "^10.4.5",
        "@types/geojson": "^7946.0.10",
        "@types/geojson-vt": "^3.2.0",
        "@types/terraformer__arcgis": "^2.0.2",
        "maplibre-gl": "^2.4.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "rollup-plugin-web-worker-loader": "^1.6.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@terraformer/arcgis": "^2.1.2",
        "rollup-plugin-import-css": "^3.1.0"
    }
}
